<#IdentificationMapping>
    rml:logicalSource [ 
            rml:sourceName "HelloWorldPerson.csv";
            rml:queryLanguage ql:CSV
    ];

    rr:subjectMap [
        rr:template "http://ewi.mmlab.be/cd/Identification/{Identifier}"; 
        #Which is the correct class?
        rr:class oslo:Identification;
    ];
    rr:predicateObjectMap [
        rr:predicate skos:notation;
        rr:objectMap [ 
            rml:reference "Identifier";
	];
    ];
        
    rr:predicateObjectMap [
        rr:predicate dcterms:type;
        rr:objectMap [ 
            rml:reference "IdentifierType";
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate dcterms:created;
        rr:objectMap [ 
            rml:reference "IssueDate";
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate adms:schemeAgency;
        rr:objectMap [ 
            rml:reference "IssuingAuthority";
        ];
    ];

    rr:predicateObjectMap [
        rr:predicate dcterms:creator;
        rr:objectMap [ 
            rml:reference "IssuingAuthorityID";
        ];
    ].


