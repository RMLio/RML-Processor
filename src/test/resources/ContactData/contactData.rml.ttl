<#PersonMapping>
    rml:logicalSource [ 
            rml:sourceName "HelloWorldPerson.csv";
            rml:queryLanguage ql:CSV
    ];
    #should be identifier
    rr:subjectMap [
        rr:template "http://ewi.mmlab.be/cd/Person/{Naam}+{Voornaam}"; 
        rr:class person:Person;
    ];
    rr:predicateObjectMap [
        rr:predicate person:familyName;
        rr:objectMap [ 
            rml:reference "Naam";
	];
    ];
        
    rr:predicateObjectMap [
        rr:predicate person:givenName;
        rr:objectMap [ 
            rml:reference "Voornaam";
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate person:patronymicName;
        rr:objectMap [ 
            rml:reference "Andere voornamen" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate schema:gender;
        rr:objectMap [ 
            rml:reference "Geslacht" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate oslo:nobelityTitle;
        rr:objectMap [ 
            rml:reference "Adellijke titel" 
        ];
    ];
        
    rr:predicateObjectMap [
        #weird values for birth dates on the file
        rr:predicate schema:birthDate;
        rr:objectMap [ 
            rml:reference "Geboortedatum" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate person:placeOfBirth;
        rr:objectMap [ 
            rml:reference "Geboorteplaats" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate schema:deathDate;
        rr:objectMap [ 
            rml:reference "Datum overlijden" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate person:placeOfDeath;
        rr:objectMap [ 
            rml:reference "Plaats overlijden" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate schema:nationality;
        rr:objectMap [ 
            rml:reference "Nationaliteit" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate oslo:legalCohabetation;
        rr:objectMap [ 
            rml:reference "Wettelijk samenwonend" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate oslo:civilClass;
        rr:objectMap [ 
            rml:reference "Burgerlijke staat" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate oslo:legalIncompetence;
        rr:objectMap [ 
            rml:reference "Juridische onbewkwaamheid" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate oslo:domicile;
        rr:objectMap [ 
            rml:reference "Is gedomicilieerd op" 
        ];
    ];
        
    rr:predicateObjectMap [
        rr:predicate oslo:residence;
        rr:objectMap [ 
            rml:reference "Verblijft" 
        ];
    ].
   
