@prefix rr:     <http://www.w3.org/ns/r2rml#>.
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix mail:   <http://example.com/mail#>.
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#>.
@prefix adms:   <http://www.w3.org/ns/adms#> .
@prefix dcterms:<http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .
@prefix oslo:   <http://purl.org/oslo/ns/localgov#> .
@prefix person: <http://www.w3.org/ns/person/> .
@prefix ecd:    <http://ewi.mmlab.be/cd> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix rov:    <http://www.w3.org/ns/regorg#> .
@prefix prov:   <http://www.w3.org/ns/prov#> .
@prefix locn:   <http://www.w3.org/ns/locn#> .
@prefix org:    <http://www.w3.org/ns/org#> .
@base           <http://ewi.mmlab.be/cd> .

<#PersonMapping>
    rml:logicalSource [ 
            rml:sourceName "contactDataFullEx.csv";
            rml:queryLanguage ql:CSV
    ];
    #should be identifier
    rr:subjectMap [
        rr:template "http://ewi.mmlab.be/cd/Person/{identifier}"; 
        rr:class person:Person;
    ];
    rr:predicateObjectMap [
        #check if this property exists, if not which one is the correct?
        rr:predicate person:name;
        rr:objectMap [ 
            rml:reference "person";
	];
    ];
    rr:predicateObjectMap [
        #check if this property exists, if not which one is the correct?
        rr:predicate schema:email;
        rr:objectMap [ 
            rml:reference "contact";
	];
    ];
    rr:predicateObjectMap [
        rr:predicate skos:notation;
        rr:objectMap [ 
            rml:reference "identifier";
	];
    ].

<#OrganisationMapping>
    rml:logicalSource [ 
            rml:sourceName "contactDataFullEx.csv";
            rml:queryLanguage ql:CSV
    ];
    rr:subjectMap [
        rr:template "http://ewi.mmlab.be/cd/Organisation/{organisatie_identifier}"; 
        #Is this class ok?
        rr:class oslo:Organisation;
    ];
    rr:predicateObjectMap [
        #is this ok?
        rr:predicate skos:notation;
        rr:objectMap [ 
            rml:reference "organisatie_identifier";
	];
    ];

    rr:predicateObjectMap [
        rr:predicate rov:legalName;
        rr:objectMap [ 
            rml:reference "organisatie";
	];
    ].

